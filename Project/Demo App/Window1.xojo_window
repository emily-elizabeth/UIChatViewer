#tag Events UIChatViewer1
	#tag Event
		Sub Opening()
		  me.AppendChat DateTime.Now, 1, "User", Nil, "So a priest, rabbi, and a chicken walk in to a bar.", FALSE
		  me.AppendChat DateTime.Now, 2, "Emily", Nil, "I'm pretty sure I've heard this one before", TRUE
		  me.AppendChat DateTime.Now, 2, "Emily", Nil, "So what happens next?", TRUE
		  me.AppendChat DateTime.Now, 1, "User", Nil, "If I remember correctly, they explode outward at the speed of light.", FALSE
		  me.AppendChat DateTime.Now, 1, "User", Nil, "But that might be if you cross the streams...", FALSE
		  me.AppendChat DateTime.Now, 2, "Emily", Nil, "... thus negating all existence!", TRUE
		  me.AppendChat DateTime.Now, 1, "User", Nil, "Exactly! It's a risk one takes whenever one walks in to a bar, especially if one is a chicken.", FALSE
		  me.AppendNotification DateTime.Now, "away", "Emily went away"
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton1
	#tag Event
		Sub Action()
		  Var f As FolderItem = Folderitem.ShowSelectFolderDialog
		  If f <> Nil Then
		    self.UIChatViewer1.MessageStylePath = f
		    self.UIChatViewer1.Refresh
		  End If
		End Sub
	#tag EndEvent
